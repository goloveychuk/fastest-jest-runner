{
  "name": "fastest-jest-runner",
  "version": "29.0.2",
  "repository": {
    "type": "git",
    "url": "https://github.com/goloveychuk/fastest-jest-runner.git",
    "directory": "runner"
  },
  "license": "MIT",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "gypfile": true,
  "files": [
    "addon",
    "binding.gyp",
    "dist",
    "package.json"
  ],
  "scripts": {
    "build:addon": "node-gyp rebuild",
    "build": "yarn run build:addon && rm -rf dist && tsc",
    "watch": "rm -rf dist && tsc --watch",
    "publish": "yarn npm publish"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./package.json": "./package.json"
  },
  "dependencies": {
    "@jest/console": "^29.0.0",
    "@jest/environment": "^29.0.0",
    "@jest/test-result": "^29.0.0",
    "@jest/transform": "^29.0.0",
    "@jest/types": "^29.0.0",
    "@types/node": "14.14.45",
    "chalk": "^4.0.0",
    "co": "^4.6.0",
    "emittery": "^0.10.2",
    "graceful-fs": "^4.2.9",
    "is-generator-fn": "^2.0.0",
    "jest-circus": "^29.0.0",
    "jest-config": "^29.0.0",
    "jest-docblock": "^29.0.0",
    "jest-environment-node": "^29.0.0",
    "jest-haste-map": "^29.0.0",
    "jest-leak-detector": "^29.0.0",
    "jest-message-util": "^29.0.0",
    "jest-resolve": "^29.0.0",
    "jest-runtime": "^29.0.0",
    "jest-util": "^29.0.0",
    "jest-watcher": "^29.0.0",
    "jest-worker": "^29.0.0",
    "node-addon-api": "^5.0.0",
    "node-gyp": "^9.1.0",
    "p-limit": "^3.1.0",
    "source-map-support": "0.5.13"
  },
  "devDependencies": {
    "@types/co": "^4.6.2",
    "@types/exit": "^0.1.30",
    "@types/graceful-fs": "^4.1.3",
    "@types/source-map-support": "^0.5.0",
    "jest-jasmine2": "^29.0.0",
    "typescript": "^4.8.2"
  },
  "engines": {
    "node": "^14.15.0 || ^16.10.0 || >=18.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
